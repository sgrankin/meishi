{
  "$schema": "vscode://schemas/color-theme",
  "name": "meishi (light)",
  "type": "light",
  "colors": {
    "editor.selectionHighlightBackground": "#99ff99",
    "editor.selectionHighlightBorder": "#009900",
    "editorBracketPairGuide.activeBackground1": "#888888",
    "editorBracketPairGuide.activeBackground2": "#888888",
    "editorBracketPairGuide.activeBackground3": "#888888",
    "editorBracketPairGuide.activeBackground4": "#888888",
    "editorBracketPairGuide.activeBackground5": "#888888",
    "editorBracketPairGuide.activeBackground6": "#888888",
    "activityBar.background": "#000000",
    "editor.lineHighlightBackground": "#eaeaea",
    "sideBar.foreground": "#000000",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBrightBlack": "#000000",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.ansiWhite": "#e9ebeb",
    "terminal.foreground": "#000000",
  },
  "tokenColors": [
    { "scope": [ "source" ], "settings": { "foreground": "#000000", "fontStyle": "" } },
    { "scope": [ "comment", "punctuation.definition.comment" ], "settings": { "foreground": "#008000", "fontStyle": "" } },
    { "scope": [ "entity.name.type.namespace", "support.package" ], "settings": { "foreground": "#000000", "fontStyle": "" } },
    { "scope": [ "constant", "keyword", "storage.modifier", "storage.type.class", "storage.type.function", "storage.type.import", "storage.type.package", "storage.type.js", "storage.type.ts" ], "settings": { "foreground": "#0000FF", "fontStyle": "" } },
    { "scope": [ "entity.name.type", "storage.type", "support.class" ], "settings": { "foreground": "#C000C0", "fontStyle": "" } },
    { "scope": [ "constant.numeric" ], "settings": { "foreground": "#FF0000", "fontStyle": "" } },
    { "scope": [ "punctuation.definition.string", "string", "string.quoted.docstring" ], "settings": { "foreground": "#A31515", "fontStyle": "" } },
    { "scope": [ "support.function.builtin" ], "settings": { "foreground": "#0000FF", "fontStyle": "bold" } },
    { "scope": [ "entity.name.function", "meta.method", "support.function" ], "settings": { "foreground": "#000000", "fontStyle": "bold" } },
    { "scope": [ "variable" ], "settings": { "foreground": "#000000", "fontStyle": "" } },
    { "scope": [ "constant.other.placeholder.go" ], "settings": { "foreground": "#FF0000", "fontStyle": "" } },
    { "scope": [ "punctuation.definition.tag", "entity.name.tag" ], "settings": { "foreground": "#0000FF", "fontStyle": "bold" } },
    { "scope": [ "entity.other.attribute-name" ], "settings": { "foreground": "#C000C0", "fontStyle": "" } },
    { "scope": "token.info-token", "settings": { "foreground": "#316BCD" } },
    { "scope": "token.warn-token", "settings": { "foreground": "#CD9731" } },
    { "scope": "token.error-token", "settings": { "foreground": "#CD3131" } },
    { "scope": "token.debug-token", "settings": { "foreground": "#800080" } },
    { "scope": [ "markup.heading", "punctuation.definition.list" ], "settings": { "fontStyle": "bold" } },
    { "scope": [ "markup.fenced_code.block", "markup.inline.raw.string" ], "settings": { "foreground": "#008000" } },
    { "scope": [ "markup.bold", ], "settings": { "fontStyle": "bold" } },
    { "scope": [ "markup.italic", ], "settings": { "fontStyle": "italic" } },
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "*": { "foreground": "#090909" },
    // Semantic token classification:
    // Modules
    "namespace": { "foreground": "#003D67" },
    // Nominal Types
    "type": { "foreground": "#C000C0" },
    "class": { "foreground": "#C000C0" },
    "enum": { "foreground": "#C000C0" },
    "interface": { "foreground": "#C000C0" },
    "struct": { "foreground": "#C000C0" },
    "typeParameter": { "foreground": "#003D67", "italic": true },
    // Variables
    "parameter": { },
    "variable": { },
    "property": { },
    "enumMember": { },
    "event": { },
    // Callables
    "function": { "bold": true },
    "method": { "bold": true },
    "macro": { "bold": true },
    // Labels
    "label": { },
    // Syntax
    "comment": { "foreground": "#008000", "italic": false },
    "string": { "foreground": "#A31515", "italic": true },
    "keyword": { "foreground": "#0000FF" },
    "number": { "foreground": "#FF0000" },
    "regexp": { "foreground": "#A31515" },
    "operator": { },
    // Semantic token modifiers:
    // Declaration
    "*.declaration": { },
    // Attributes
    "*.readonly": { },
    "*.static": { },
    "*.deprecated": { "underline": true },
    "*.abstract": { },
    // Usage?
    "*.async": { },
    "*.modification": { },
    "*.documentation": { },
    "*.defaultLibrary": { },
    // Rust specific:
    "*.attribute:rust": { },
    "attribute:rust": { },
    "boolean:rust": { }, // keyword
    "builtinType:rust": { }, // type
    "lifetime:rust": { /* "foreground": "#C000C0", */ "foreground": "#003D67", "italic": true },
    "selfKeyword:rust": { }, // keyword
    "typeAlias:rust": { }, // type
    "union:rust": { }, // type
    "formatSpecifier:rust": { "foreground": "#0000FF" },
    "enumMember:rust": { /* Enum members are constructors, so should look like callables. */ "bold": true },
    "unresolvedReference:rust": { "foreground": "#ff0000" },
    // C++
    "*.global:cpp": { "underline": true }
  }
}
