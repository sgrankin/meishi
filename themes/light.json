    "tokenColors": [
        {
            "scope": [
                "source"
            ],
            "settings": {
                "foreground": "#000000",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#008000",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "entity.name.type.namespace",
                "support.package"
            ],
            "settings": {
                "foreground": "#000000",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "constant",
                "keyword",
                "storage.modifier",
                "storage.type.class",
                "storage.type.function",
                "storage.type.import",
                "storage.type.package",
                "storage.type.js",
                "storage.type.ts"
            ],
            "settings": {
                "foreground": "#0000FF",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "entity.name.type",
                "storage.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#C000C0",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#FF0000",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "punctuation.definition.string",
                "string"
            ],
            "settings": {
                "foreground": "#A31515",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "support.function.builtin"
            ],
            "settings": {
                "foreground": "#0000FF",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "meta.method",
                "support.function"
            ],
            "settings": {
                "foreground": "#000000",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#000000",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "constant.other.placeholder.go"
            ],
            "settings": {
                "foreground": "#FF0000",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "punctuation.definition.tag",
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#0000FF",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#C000C0",
                "fontStyle": ""
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#316BCD"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#CD9731"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#CD3131"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#800080"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "*": {
            "foreground": "#090909"
        },
        // Semantic token classification:
        // Modules
        "namespace": {
            "foreground": "#003D67"
        },
        // Nominal Types
        "type": {
            "foreground": "#C000C0"
        },
        "class": {
            "foreground": "#C000C0"
        },
        "enum": {
            "foreground": "#C000C0"
        },
        "interface": {
            "foreground": "#C000C0"
        },
        "struct": {
            "foreground": "#C000C0"
        },
        "typeParameter": {
            "foreground": "#003D67",
            "italic": true
        },
        // Variables
        "parameter": {},
        "variable": {},
        "property": {},
        "enumMember": {},
        "event": {},
        // Callables
        "function": {
            "bold": true
        },
        "member": {
            "bold": true
        },
        "macro": {
            "bold": true
        },
        // Labels
        "label": {},
        // Syntax
        "comment": {
            "foreground": "#008000",
            "italic": true
        },
        "string": {
