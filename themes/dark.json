{
  "$schema": "vscode://schemas/color-theme",
  "name": "meishi (dark)",
  "type": "dark",
  "colors": {
    "editor.selectionHighlightBackground": "#003D67",
    "editor.selectionHighlightBorder": "#004488",
    "editorBracketPairGuide.activeBackground1": "#888888",
    "editorBracketPairGuide.activeBackground2": "#888888",
    "editorBracketPairGuide.activeBackground3": "#888888",
    "editorBracketPairGuide.activeBackground4": "#888888",
    "editorBracketPairGuide.activeBackground5": "#888888",
    "editorBracketPairGuide.activeBackground6": "#888888",
    "editor.background": "#1f1f1d",
    "editor.foreground": "#f9f6ef",
    "symbolIcon.classForeground": "#00a4ef",
    "terminal.ansiBlack": "#555555",
    "terminal.ansiBlue": "#409cff",
    "terminal.ansiBrightBlack": "#b3b3b3",
    "terminal.ansiBrightBlue": "#00a4ef",
    "terminal.ansiBrightCyan": "#66d9ef",
    "terminal.ansiBrightGreen": "#7fba00",
    "terminal.ansiBrightMagenta": "#a97cf8",
    "terminal.ansiBrightRed": "#f25022",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.ansiBrightYellow": "#ffb900",
    "terminal.ansiCyan": "#70d7ff",
    "terminal.ansiGreen": "#a9dc76",
    "terminal.ansiMagenta": "#a89fec",
    "terminal.ansiRed": "#ee7762",
    "terminal.ansiWhite": "#f9f6ef",
    "terminal.ansiYellow": "#ffb900",
    "terminal.background": "#1f1f21",
    "terminal.foreground": "#f9f6ef",
    "terminal.selectionBackground": "#42557b5a"
  },
  "tokenColors": [
    { "scope": "comment", "settings": { "foreground": "#a0a0a0", "fontStyle": "italic", } },
    { "scope": "keyword", "settings": { "foreground": "#F38230" } },
    { "scope": "variable", "settings": { /* "foreground": "#D4CCFF"*/ } },
    { "scope": "variable.parameter", "settings": { /* "foreground": "#FBB73B" */ } },
    { "scope": "constant", "settings": { "foreground": "#FBB73B" } },
    { "scope": "meta.function-call", "settings": { /* "foreground": "#64B94D"*/ } },
    { "scope": "meta.function", "settings": { /* "foreground": "#64B94D" */ } },
    { "scope": "meta.type", "settings": { "foreground": "#70D7FF" } },
    { "scope": "meta.type.annotation", "settings": { /* "foreground": "#FFB900" */ } },
    { "scope": "constant.numeric", "settings": { "foreground": "#70D7FF" } },
    { "scope": "token.info-token", "settings": { "foreground": "#6796E6" } },
    { "scope": "token.warn-token", "settings": { "foreground": "#CD9731" } },
    { "scope": "token.error-token", "settings": { "foreground": "#F44747" } },
    { "scope": "token.debug-token", "settings": { "foreground": "#B267E6" } },
    { "scope": "markup.heading", "settings": { "fontStyle": "bold" } },
    // { "scope": "string", "settings": { "foreground": "#FFD866", "fontStyle": "italic", } },
    { "scope": "string", "settings": { "fontStyle": "" } },
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "*": { "foreground": "#F9F6EF" },
    // Semantic token classification:
    // Modules
    "namespace": { "foreground": "#FBB73B" },
    // Nominal Types
    "type": { "foreground": "#70D7FF" },
    "class": { "foreground": "#70D7FF" },
    "enum": { "foreground": "#70D7FF" },
    "interface": { "foreground": "#70D7FF" },
    "struct": { "foreground": "#70D7FF" },
    "typeParameter": { "foreground": "#64B94D" },
    // Variables
    "parameter": { /* "foreground": "#FBB73B" */ },
    "variable": { /* "foreground": "#D4CCFF" */ },
    "property": { /* "foreground": "#D4CCFF" */ },
    "enumMember": { /* "foreground": "#D4CCFF" */ },
    "event": { /* "foreground": "#D4CCFF" */ },
    // Callables
    "function": { },
    "macro": { /* "bold": true, */ },
    "method": { "bold": true },
    // Labels
    "label": { },
    // Syntax
    "comment": { "foreground": "#A0A0A0", "italic": true },
    "string": { "foreground": "#FFD866", "italic": true },
    "keyword": { "foreground": "#F38230" },
    "number": { "foreground": "#70D7FF" },
    "regexp": { "foreground": "#FFD866" },
    "operator": { },
    // Semantic token modifiers:
    // Declaration
    "*.declaration": { },
    // Attributes
    "*.readonly": { },
    "*.static": { },
    "*.deprecated": {
      "underline": true
    },
    "*.abstract": { },
    // Usage?
    "*.async": { },
    "*.modification": { },
    "*.documentation": { },
    "*.defaultLibrary": { },
    // Rust specific:
    "attribute:rust": { "foreground": "#F38230" },
    "*.attribute:rust": { "foreground": "#F38230" },
    "boolean:rust": { }, // keyword
    "builtinType:rust": { }, // type
    "lifetime:rust": { "foreground": "#64B94D" },
    "selfKeyword:rust": { }, // keyword
    "typeAlias:rust": { }, // type
    "union:rust": { }, // type
    "formatSpecifier:rust": { "foreground": "#F38230" },
    "enumMember:rust": { /* Enum members are constructors, so should look like callables. */ },
    "unresolvedReference:rust": { "foreground": "#DE3C43"
    },
    // C++
    "*.global:cpp": { "underline": true },
    // Other?
    "*.constant": { "foreground": "#FBB73B" }
  }
}
